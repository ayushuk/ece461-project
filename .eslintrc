{
  "extends": ["oclif", "oclif-typescript", "prettier"],
  "rules": {
    "arrow-parens": "off",
    "indent": "off",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": [
      "error",
      { "argsIgnorePattern": "^_" }
    ] // add a underscore to remove the ignore unused variables error
  }
}
